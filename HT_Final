#Ejercicio1
print("EJERCICIO #1")
Password=input("Defina una contraseña: ")
name=input("ingresar la contraseña: ")

while Password!=name:
    name=input("ingresar contraseña: ")
    if Password != name:
        print("acceso Denegado")

while Password == name:

        print("acceso permitido")

        break

#Ejercicio2
print("EJERCICIO #2")
nombre = input("Ingrese su nombre: ")
gender = input("Cual es su genero? M o H ")

if gender == "M":
    if nombre.lower() < "m":
        group = "A"
    else:
        group = "B"

else:

    if nombre.lower() > "n":
        group = "A"
    else:
        group = "B"
print("Tu grupo es: "+ group)

